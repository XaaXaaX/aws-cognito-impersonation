{
  "name": "cdk",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk:app": "cdk --app 'ts-node --prefer-ts-exts ./bin/app.ts'",
    "cdk:app:dev": "npm run cdk:app -- --profile serverless -c env=dev"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.30",
    "aws-cdk": "^2.137.0",
    "esbuild": "^0.20.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "aws-cdk-lib": "^2.137.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  }
}
